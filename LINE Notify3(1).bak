Function BIG5(text)
	set objStream = Createobject("Adodb.Stream")
	with objStream
		.Type = 1
		.Mode = 3
		.Open
		.Write text
		.Position = 0
		.Type = 2
		.Charset = "UTF-8"
		BIG5 = .ReadText
		.Close
	End with
End Function
