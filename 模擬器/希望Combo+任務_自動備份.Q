Sub 拔草()
    Call 大漠設定()
    Delay 100
    選項X = array(700,1050,700,1050)
    選項Y = array(530, 530, 630, 630)
    初始_Unix = 時間
    任務時間 = 時間
    Do
    	If Form1.任務.Value = 1 And 時間 - 任務時間 > int(Form1.任務超時.Text) Then 
    		Call 結束所有APP
    	End If
    	'檢測是否戰鬥介面
        dm_ret = dm.FindStr(1210, 7, 1265, 62, "錯誤", "f5f4ed-303030", 0.7, intX, intY)
        If intX > 0 Then
			Unix = 時間
        	'啟動連續技
            If Form1.連續技.Value = 1 Then 
                If Form1.Combo.Value = 1 Then 
                    'combo
                    dm_ret = dm.FindColor(1073,583,1074,600 + (Form1.功速.ListIndex * 4  ),"db3ffb-505050",1.0,0,intX,intY)
                    If intX >= 0 and intY >= 0 Then 
                        Call 鍵盤點擊(32, 400)
                    End If
                End If
                If Form1.技能.Value = 1 Then 
                    'skill
                    dm_ret = dm.FindColor(1240,528,1245,537,"fffffb-000010",1.0,0,intX,intY)
                    If intX >= 0 and intY >= 0 Then
                        Call 鍵盤點擊(49, 100)
                    End If
                End If
                If Form1.Fever.Value = 1 Then 
                    'Ferver
                    dm_ret = dm.FindColor(957,519,968,524,"fffffb-000010",1.0,0,intX,intY)
                    If intX >= 0 and intY >= 0 Then
                        Call 鍵盤點擊(50, 100)
                    End If
                End If
            End If
            '啟動任務
            If 初始_Unix <= Unix Then 
            	If Form1.任務.Value = 1  Then 
                	Call 確認任務()
            	End If
            	'啟動分解
            	If Form1.分解.Value = 1 Then 
                	Call 分解()
            	End If
            	初始_Unix = Unix + 2
            End If
        Else '沒有戰鬥介面
			Delay 200
        	'錯誤排除
            dm_ret = dm.FindStr(609, 491, 760, 515, "確認", "ffffff-303030", 0.7, intX, intY)
            If intX > 0 Then 
            	call 滑鼠點擊(intX, intY, 0)
            Else 
            	Delay 100
                dm_ret = dm.FindStr(60, 1, 151, 46, "[委託處]", "EBECEE-141311", 0.7, intX, intY)
                If intX > 0 Then 
                    If Form1.任務.Value = 1 Then 
                        Call 任務(1234, 31)
                    End If 
                Else
            		Delay 100
                   	dm_ret = dm.FindStr(1210, 30, 1251, 71, "關閉", "ffffff-404040", 0.7, intX, intY)
                    If dm_ret = 0 Then 
                    //dm_ret = dm.FindStr(1206, 3, 1266, 63, "錯誤", "656462-151515|363635-151515|212020-151515", 0.63, intX, intY)
                    	TracePrint "關閉"
                    	Call 滑鼠點擊(intX, intY, 0)
                    Else 
                    	dm_ret = dm.FindStr(1206, 3, 1266, 80, "錯誤", "737270-535251", 0.73, intX, intY)
                    	dm_ret = dm.FindStr(1206, 3, 1266, 80, "錯誤", "A7A6A3-101010|363635-000000", 0.73, intX2, intY2)
                    	If intX > 0 or intX2 > 0 Then 
                    		TracePrint "錯誤"
                    		call 滑鼠點擊(1200,35, 0)
                    	Else
                    		dm_ret = dm.FindStr(1135, 35, 1185, 62, "Skip", "ffffff-202020", 0.8, intX, intY)
                    		If dm_ret = 0 Then 
                    			TracePrint "Skip"
                    			call 滑鼠點擊(1200,35, 0)
                    		Else 
                    			dm_ret = dm.FindStr(1208,15,1267,75, "錯誤", "ffffff-202020", 0.75, intX, intY)
                    			If intX > 0 Then 
                    				TracePrint "選單"
                    				call 滑鼠點擊(intX,intY, 1000)
                    			Else 
                    				'閃退重新登入
            						Delay 100
            						dm_ret = dm.FindPic(997,2,1219,31,"WIFI.bmp","000000",0.9,0,intX,intY)
                					If Form1.登入.Value = 1 And intX > 0 Then 
                						dm_ret = dm.FindStr(0, 0, 1280, 720, "希望M", "ffffff-303030", 0.9, intX, intY)
                						If intX > 0 Then 
                    						Call 登入
                 						End If
									End If
                    			End If
                			End If
                		End If
                    End If
                End If
            End If
            Delay 1000
        End If
        Delay 10
    Loop
End Sub

Sub 確認任務()
    dm_ret = dm.FindStr(18, 281, 53, 315, "Quset", "ffffff-303030", 0.9, intX1, intY1)
    Delay 10
    dm_ret = dm.FindStr(170, 290, 227, 315, "完成", "DCC137-303030|F7F4BE-080B41", 0.65, intX, intY)
    If intX1 > 0 Then 
    	'任務完成
        If intX > 0 Then 
            TracePrint "完成任務"
            Form1.狀態.Caption = "【完成任務】"
			Call 任務(intX, intY)
        Else 
        	If Form1.不指定任務.Value = 0 Then
            	'指定任務
            	dm_ret = dm.FindColor(18,281,53,315, "DBF961-17064C", 0.99, 0, intX, intY)
            	If intX = -1  Then 
                	Form1.狀態.Caption = "(指定任務)"
                	TracePrint "指定任務"
                	call 滑鼠點擊(62,275, 500)
            	End If
            Else 
        		Form1.狀態.Caption = "啟動AUTO"
        		dm_ret = dm.FindStr(1180, 623, 1225, 639, "AUTO", "80e3f2-202020", 0.8, intX, intY)
        		If intX = - 1  Then 
     	   			call 滑鼠點擊(1200, 630, 1000)
        		End If
            End If
        End If
    ElseIf intX = - 1  Then
		TracePrint "[接任務]"
		Call 任務(1234, 31)
    End If
End Sub

