Sub 窗口
	hwnds = dm.EnumWindow(0, "", "LDPlayerMainFrame", 2 + 4 + 8 + 16)
	hwnds = split(hwnds, ",")
	列表內容 = "請選擇"
	For i = 0 To UBound(hwnds)
		title = dm.GetWindowTitle(hwnds(i))
		For ii = 1 To UBound(Split(Form1.選單.List, "|"))
			If title = Split(Split(Form1.選單.List, "|")(ii), ",")(0) Then 
				相同 = True
				Exit For
			Else 
				相同 = False
			End If
		Next
		
		If 相同 = True Then 
			列表內容 = 列表內容 & "|" & Split(Form1.選單.List, "|")(ii)
		Else 
			模擬器 = dm.EnumWindow(hwnds(i), "sub", "subWin", 1 + 2)
			列表內容 = 列表內容 & "|" & title & ",0"
		End If
	Next
	Form1.選單.List = 列表內容
	Form1.選單.ListIndex = 0
End Sub

