sub 任務(qx, qy)
	'取消AUTO
    If Form1.取消AUTO.Value = 1 Then 
        Form1.狀態.Caption = "取消AUTO"
        dm_ret = dm.FindStr(1180, 623, 1225, 639, "AUTO", "80e3f2-202020", 0.8, intX, intY)
        If intX > 0 Then 
     	   call 滑鼠點擊(intX+20, intY, 1000)
        End If
    End If
    '點開列表
    Form1.狀態.Caption = "取得任務"
    Call 滑鼠點擊(qx, qy, 1000)
    For 5
    	'進入委託所
    	dm_ret = dm.FindStr(876,536,1188,556, "委|託|處", "99a1ad-202020", 0.7, intX, intY)
    	If intX > 0 Then 
        	TracePrint "委託處"
        	Call 滑鼠點擊(intX, intY, 2000)
        	Exit For
        Else 
        	Delay 200
    	End If
    Next
    '確認進入委託所
    dm_ret = dm.FindStr(69,11,149,38, "[委託處]", "EBECEE-141311", 0.75, intX, intY)
    If intX > 0 Then 
    	Call 任務城鎮
    	Call 任務難度
		Call 任務項目
        Form1.狀態.Caption = "結束任務"
        任務時間 = 時間
    End If
End Sub

Sub 任務城鎮
    '選擇任務城鎮
    城鎮 = Split(Form1.城鎮.List, "|")
	TracePrint 城鎮(Form1.城鎮.ListIndex)
	s=0
    Do
        dm_ret = dm.FindStr(201,133,361,671, 城鎮(Form1.城鎮.ListIndex), "ffffff-303030", 0.7, intX, intY)
        If intX > 0 Then 
        	'TracePrint intX &", "&intY
        	Form1.狀態.Caption = 城鎮(Form1.城鎮.ListIndex) & "MovetoX " & intX & " MovetoY " & intY
        	For 2
        		call 滑鼠點擊(intX, intY, 500)
        	Next
            Exit Do
        Else 
            If Form1.城鎮.ListIndex < 1 Then 
                TracePrint "向上拉"
                call 滑動(300, 400, 600, 2)
            ElseIf Form1.城鎮.ListIndex > 2 Then
                TracePrint "向下拉"
                call 滑動(300, 600, 400, -2)
            End If
            Delay 1000
        End If
        Delay 100
        s=s+1
        TracePrint "尋找" & s
    Loop While intX = - 1 
End Sub

Sub 任務難度
    '選擇難易度
    難度 = Split(Form1.難度.List, "|")
	TracePrint 難度(Form1.難度.ListIndex)
	For 2
		dm_ret = dm.FindStr(743,68,1148,101, 難度(Form1.難度.ListIndex), "7d7d7d-303030", 0.7, intX, intY)
		If intX > 0 Then 
			Form1.狀態.Caption = 難度(Form1.難度.ListIndex) & "MovetoX " & intX & " MovetoY " & intY
			Call 滑鼠點擊(intX, intY, 1000)
		End If
	Next
End Sub

Sub 任務項目
	'選擇任務
	'檢查"委託處任務"位置
	quest = dm.FindStr(649,232,759,274, "委託處任務", "f5f4ed-303030", 0.75, intX, intY)
	tempX = intX : tempY = intY
	TracePrint "委託處任務 " & quest
	s = 0 : ss = 0
	怪物= Split(Form1.選項.List,"|") 
	行數 = int(Form1.選項.ListIndex / 2)
	Quest_s = int(Form1.選項.ListIndex) Mod 2
	temp_行數 = -1

	'滑動
	TracePrint "需檢測到委託 " & 行數
	Form1.狀態.Caption = "需檢測到委託 " & 行數
	檢查迴圈 = 0
	Do While ss <= 行數 And 檢查迴圈 <= 行數
		'判斷第幾行任務
		If 行數 = 0 Then 
			'需要向上滑動
			If quest = 0 Then 
				TracePrint "向上拉"
				Form1.狀態.Caption = "向上拉"
				Call 滑動(875, 250, 600, 1)
				Delay 1000
			End If
			Exit Do
		ElseIf 行數 > 0 Then
			'判斷起始點下參數
			If quest = 0 Then 
				temp_行數 = 行數 - 1 : quest = 1
			ElseIf quest = -1 Then 
				temp_行數 = 行數 : quest = 1
			End If
			TracePrint "temp_行數 " & temp_行數
			
			If temp_行數 = 0 or temp_行數 = ss Then 
				If temp_行數 = 0 Then 
					tempY = tempY +20
				End If
				Exit Do
			'任務暫存行數大於0
			ElseIf temp_行數 > 0 Then 
				TracePrint "向下拉"
				Form1.狀態.Caption = "向下拉"
				If 檢查數 >= 50 Then 
					Call 滑動(875, 600, 400, - 1 )
				Else 
					Call 滑動(875, 600, 250, - 1 )
				End If
				
				'迴圈檢測到委託
				檢查數 = 0
				Do
					檢查數 = 檢查數 + 1
					dm_ret = dm.FindStr(655,130,759,280, "委託處任務", "f5f4ed-303030", 0.75, intX, intY)
					If intX > 0 Then 
						ss = ss + 1
						Call 滑鼠點擊(875, intY, 200)
						If ss = temp_行數 Then 
							Delay 200
							'重新定位
							dm_ret = dm.FindStr(655,130,759,280, "委託處任務", "f5f4ed-303030", 0.75, intX, intY)
							tempX = intX : tempY = intY + 15
						End If
						//TracePrint "tempX " & tempX & " tempY " & tempY
						Exit Do
					Else 
						Delay 2
					End If
				Loop While 檢查數 < 50
				
				TracePrint "檢測到次數: " & ss
				Form1.狀態.Caption = "檢測到委託: " & ss
			End If
		End If
		檢查迴圈 = 檢查迴圈 + 1
	Loop
	'移動任務座標
	TracePrint "選擇任務 " & 怪物(Form1.選項.ListIndex)
	Form1.狀態.Caption = "選擇任務 " & 怪物(Form1.選項.ListIndex)
	If 行數 = 0 Then 
		If Quest_s = 0 Then 
			TracePrint "點擊任務左上"
			Form1.狀態.Caption = "點擊任務右下"
			call 滑鼠點擊(700, 530, 0)
		Else
			TracePrint "點擊任務右上"
			Form1.狀態.Caption = "點擊任務右下"
			call 滑鼠點擊(1050, 530, 0)
		End If
	Else 
		If Quest_s = 0 Then 
			TracePrint "點擊任務左下"
			Form1.狀態.Caption = "點擊任務左下"
			Call 滑鼠點擊(tempX + 50, tempY + 370, 0)
		Else
			TracePrint "點擊任務右下"
			Form1.狀態.Caption = "點擊任務右下"
			Call 滑鼠點擊(tempX + 50 + 365, tempY + 370, 0)
		End If
	End If
	Delay 500
	'離開委託所
	call 滑鼠點擊(35, 30, 1000)
	Form1.狀態.Caption = "關閉委託所"  
End Sub

Sub 滑動(移動X, 移動A, 移動B, 變量)
    If 變量 <> 0 Then 
        dm.Moveto 移動X, 移動A
        dm.LeftDown 
        Do
            移動A = 移動A + 變量
            Delay 1
            dm.Moveto 移動X, 移動A
        Loop While 移動A <> 移動B
        dm.LeftUp 
        TracePrint "滑動結束"	
    End If
End Sub

Sub 分解
	Form1.狀態.Caption = "開始分解"
	If 分解_Unix <= Unix Then 
		dm_ret = dm.FindStr(1090, 52, 1119, 68, "背包", "f5f4ed-303030", 0.9, intX, intY)
		If intX > 0 Then 
			TracePrint "點背包"
			call 滑鼠點擊(intX, intY, 1000)
		End If
		dm_ret = dm.FindStr(1099, 55, 1177, 76, "裝備分解", "f5f4ed-303030", 0.9, intX, intY)
		If intX > 0 Then 
			TracePrint "點分解"
			'選擇裝備分解
			Call 滑鼠點擊(intX, intY, 1000)
			'點擊武器分解
			TracePrint "點武器"
			Call 分解項目(831, 123)
			'點擊防具分解
			TracePrint "點防具"
			Call 分解項目(913, 123)
			'離開背包
			For 5
				dm_ret = dm.FindStr(1210, 7, 1265, 62, "錯誤", "f5f4ed-303030", 0.7, intX, intY)
				If intX > 5 Then 
					Exit For
				Else 
					Call 滑鼠點擊(32, 32, 2000)	
				End If
			Next
			分解_Unix = 時間 + (Form1.分解時間.Value * 60)
		End If
	End If
	Form1.狀態.Caption = "分解結束"
End Sub

Sub 分解項目(tackX, tackY)
	'點擊防具
	Call 滑鼠點擊(tackX, tackY, 1000)
	dm_ret = dm.FindStr(945, 395, 1115, 420, "您沒有任何項目", "f5f4ed-303030", 0.9, intX, intY)
	If dm_ret = - 1  Then 
		'點擊一鍵選擇
		Call 滑鼠點擊(1037, 677, 1000)
		'點擊確定
		Call 滑鼠點擊(744, 576, 1000)
		dm_ret = dm.FindStr(787, 177, 1263, 635, "勾", "9bec0d-202020", 0.7, intX, intY)
		TracePrint "勾" & dm_ret
		If intX > 0 Then 
			'點擊分解
			Call 滑鼠點擊(872, 677, 1000)
			dm_ret = dm.FindStr(609, 491, 760, 515, "確認", "ffffff-303030", 0.7, intX, intY)
			If intX > 0 Then 
				'點擊確定
				Call 滑鼠點擊(intX, intY, 1000)
			End If
			For 2
				call 滑鼠點擊(1237, 33, 1000)
			Next
		End If
	End If
End Sub

Sub 滑鼠點擊(tackX, tackY, timeD)
	dm.MoveTo tackX, tackY
	dm.LeftDown
	dm.LeftUp 
	Delay timeD
End Sub

Sub 鍵盤點擊(key, timeD)
	dm.KeyDown key
	dm.KeyUp key
	Delay timeD
End Sub

Function 時間()
	時間 = DateDiff("s", "1970/01/01 00:00:00", Now) - 28800
End Function

Sub 結束所有APP()
    Call 鍵盤點擊(113,1000)
    Call 滑鼠點擊(920, 80, 1000)
    登入時間 = 時間 : tack = 0
    Form1.狀態.Caption = "重新登入"
End Sub

Sub 登入
	登入時間 = 時間
    Do
        '尋找APP
        dm_ret = dm.FindStr(0, 0, 1280, 720, "希望M", "ffffff-303030", 0.9, intX, intY)
        If intX > 0 Then 
        	TracePrint "希望M" & intX & "-" & intY
        	call 滑鼠點擊(intX, intY, 100)
        Else 
            dm_ret = dm.FindStr(710, 515, 756, 545, "確認", "ffffff-303030", 0.9, intX, intY)
            If intX > 0 Then 
            	TracePrint "確認" & intX & "-" & intY
            	call 滑鼠點擊(intX, intY, 100)
            Else 
                dm_ret = dm.FindStr(521, 477, 567, 508, "略過", "ffffff-303030", 0.9, intX, intY)
                If intX > 0 Then 
                	TracePrint "略過" & intX & "-" & intY
                	call 滑鼠點擊(intX, intY, 100)
                Else 
                    If tack >= 2 Then 
                    	Call 結束所有APP
                    Else
                    	dm_ret = dm.FindStr(1168, 666, 1214, 696, "登出", "ffffff-303030", 0.9, intX, intY)
                    	If intX > 0 Then 
                    		tack = tack + 1
                    		TracePrint "登出" & intX & "-" & intY
                    		Call 滑鼠點擊(intX - 200, intY, 100)
                    	Else 
                        	dm_ret = dm.FindStr(583, 600, 696, 631, "開始遊戲", "ffdf32-303030", 0.9, intX, intY)
                        	If intX > 0 Then 
                        		TracePrint "開始遊戲" & intX & "-" & intY
                            	Call 滑鼠點擊(intX, intY, 1000 * int(Form1.登入延遲.Text))
                            	dm_ret = dm.FindStr(1210, 30, 1251, 71, "關閉", "ffffff-404040", 0.7, intX1, intY1)
								dm_ret = dm.FindStr(1210, 7, 1265, 62, "錯誤", "f5f4ed-303030", 0.7, intX2, intY2)
        						If intX2 = - 1 And intX1 = -1 Then 
     	   							Call 結束所有APP
     	   						Else 
        							任務時間 = 時間
                            		Exit Do
        						End If
                        	End If	
                    	End If
                    End If
                End If
            End If
        End If
        Delay 1000
        If 時間 - 登入時間 > int(Form1.登入超時.Text) Then 
        	Call 結束所有APP
        End If
    Loop
End Sub

Sub 大漠設定()
    set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath() & "希望M"
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "希望M.txt")

    dm_ret = dm.BindWindow(模擬器,"gdi","windows","windows",0)
    TracePrint "句柄: " & 模擬器 & " 綁定: " & dm_ret
    Form1.狀態.Caption = "句柄: " & 模擬器 & " 綁定: " & dm_ret
