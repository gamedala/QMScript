[Script]
BeginThread 拔草

Sub 拔草()
	HwndEx = Plugin.Window.SearchEx("LDPlayerMainFrame", 0, 1)
	TracePrint HwndEx
	Hwndarr = Split(HwndEx, "|")
	For i = 0 To UBound(Hwndarr)
		模擬器 = Plugin.Window.FindEx(Hwndarr(i), 0, 0, 0)
		sText = Plugin.Window.GetTextEx(Hwndarr(i), 0)
		TracePrint "模擬器 " & 模擬器 & " 標題 " & sText
	Next
End Sub

Sub 任務()
    Call 鍵盤點擊(113,1000)

    Call 滑鼠點擊(920,80,1000)
End Sub

Sub 滑鼠點擊(tackX, tackY, timeD)
	dm.MoveTo tackX, tackY
	dm.LeftDown
	dm.LeftUp 
	Delay timeD
End Sub

Sub 鍵盤點擊(key, timeD)
	dm.KeyDown key
	dm.KeyUp key
	Delay timeD
End Sub

Function 大漠設定()
    set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath()
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "希望M.txt")
    If Form1.句柄.ListIndex = 0 Then 
    	Hwnd = Plugin.Window.Search("雷電模擬器")
    	Hwnd = Split(Hwnd, "|")
    	模擬器 = Plugin.Window.FindEx(Hwnd(0), 0, 0, 0)
    Else
    	模擬器 = Plugin.Window.MousePoint()
    End If
    dm_ret = dm.BindWindow(模擬器,"gdi","windows","windows",0)
    TracePrint "句柄: " & 模擬器 & " 綁定: " & dm_ret
End Function
