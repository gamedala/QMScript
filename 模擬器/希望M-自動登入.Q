Function 時間()
    時間 = DateDiff("s", "1970/01/01 00:00:00", Now) - 28800
End Function

Function 尋圖(X1, Y1, X2, Y2, img, sim, offsetX, offsetY)
    圖片 = "Attachment:\" & img
    FindPic W1 + X1, H1 + Y1, W1 + X2, H1 + Y2, 圖片, sim, intX, intY
    If intX > 0 And intY > 0 Then 
        MoveTo intX + offsetX, intY + offsetY
        LeftClick 1
        尋圖 = True
    Else 
        尋圖 = False
    End If
End Function

Sub 模擬器設定
    '尋找雷電窗口
    Hwnd = Plugin.Window.Find("LDPlayerMainFrame", 0)
    '尋找控制窗口
    控制Hwnd = Plugin.Window.FindEx(Hwnd, 0, 0, 0)
    '解析窗口資訊
    sRect = Plugin.Window.GetWindowRect(控制Hwnd)
    '分割資訊字串
    Dim MyArray
    MyArray = Split(sRect, "|")
    W1 = Clng(MyArray(0)): H1 = Clng(MyArray(1))
    W2 = Clng(MyArray(2)): H2 = Clng(MyArray(3))
    '列印資訊
    TracePrint "模擬器Hwnd " & Hwnd & " 控制Hwnd " & 控制Hwnd
    TracePrint "窗體座標 X:" & W1 & " Y:" & H1 & "窗體大小:" & W2 - W1 & " * " & H2 - H1
End Sub
