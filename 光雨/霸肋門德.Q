Sub 霸肋門德
    Call 大漠()
    Delay 100
    裝備位置 = int(Form1.選單位置.Text)
    洞數 = int(Form1.洞數.ListIndex)
    附魔目標 = int(Form1.附魔.Text)
    call 記憶體
    Do
    	Form1.穿戴.Text = Plugin.Memory.Read32Bit(Hwnd, 穿戴)
        Form1.一洞.Text = Plugin.Memory.Read32Bit(Hwnd, 洞記憶體(1))
		Form1.二洞.Text = Plugin.Memory.Read32Bit(Hwnd, 洞記憶體(2))
		Form1.三洞.Text = Plugin.Memory.Read32Bit(Hwnd, 洞記憶體(3))
		Form1.四洞.Text = Plugin.Memory.Read32Bit(Hwnd, 洞記憶體(4))
        If Form1.穿戴.Text = 0 Then 
            dm.Keydown int(Form1.裝備按鍵.Text)
            dm.Keyup int(Form1.裝備按鍵.Text)
            Delay 20
        else
            Select Case 洞數
            Case 1
            	洞 = int(Form1.一洞.Text)
            Case 2
            	洞 = int(Form1.二洞.Text)
            Case 3
            	洞 = int(Form1.三洞.Text)
            Case 4
            	洞 = int(Form1.四洞.Text)
            End Select
            If 洞 = 附魔目標 Then 
                Do
                    Call Plugin.Media.Beep(988, 100) //蜂鳴一秒	
                Loop
            End If
            對話視窗 = Plugin.Memory.Read32Bit(Hwnd, &H00F80558)
            選項視窗 = Plugin.Memory.Read32Bit(Hwnd, &H00F8055C)
            If 洞 <> 0 AND 選項視窗 > 0 Then ' 洞數不是0且選項窗口出現
                dm_ret = dm.FindStr(0, 0, 1600, 900, "請選重置附魔裝備", "000000-000000", 1.0, intX, intY)
                dm_ret = dm.FindStr(0, 0, 1600, 900, "重置附魔需", "000000-000000", 1.0, intX2, intY2)
                If intX > 0 Then ' 選擇裝備選項
                    For 裝備位置
                        dm.Keydown 40
                        dm.Keyup 40
                        Delay 20
                    Next
                ElseIf intX2 = -1 Then ' 選擇重新附魔
                    dm.Keydown 40
                    dm.Keyup 40
                    Delay 20
                End If 
                dm.Keydown 13
                dm.Keyup 13
                Delay 20
            ElseIf 對話視窗 > 0 or 選項視窗 > 0 Then ' 與NPC對話中
                dm_ret = dm.FindStr(0, 0, 1600, 900, "請選附魔裝備", "000000-000000", 1.0, intX, intY)
                If intX > 0 AND 選項視窗 > 0 Then 
                    For 裝備位置
                        dm.Keydown 40
                        dm.Keyup 40
                        Delay 20
                    Next
                    dm.KeyPress 13
                    Delay 100
                End If
                dm.KeyPress 13
                Delay 50
            Else ' 尋找標題對話NPC
                dm_ret = dm.FindStr(0, 0, 1600, 900, "+裝備附魔", "ffffff-000000", 1.0, intX, intY)
                dm.MoveTo intX+25,intY+30
                dm.LeftClick
            End If
        End If
        Delay 50
    Loop
End Sub

Sub 記憶體
    裝備記憶體 = array(0, 18667320, 18667552, 18665464, 18666392, 18665928, 18666856, 18665696, 18666624, 18666160, 18667088, 18671960, 18672192, 18670104, 18671032, 18670568, 18671496, 18670336, 18671264, 18670800, 18671728)
    記憶體位置 = 裝備記憶體(int(Form1.位置.ListIndex))
	物品 = 記憶體位置
    穿戴 = 記憶體位置+48
    洞記憶體 = array(0,記憶體位置-16,記憶體位置-12,記憶體位置-8,記憶體位置-4)
End Sub

Function 大漠()
    set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath() & "光雨"
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "光與精煉.txt")
    'Hwnd = Plugin.Window.Find(0, "Ragnarok")
    Hwnd = dm.FindWindow("Ragnarok","") 
    dm_ret = dm.BindWindow(Hwnd, "gdi", "dx2", "dx", 4)
    TracePrint Hwnd
End Function

Event Form1.Load
    If Plugin.File.GetFileLength("c:\dm\光雨\光雨精煉.txt") = 0 Then 
        PutAttachment "c:\dm\光雨", "*.*"
    End If
End Event

Sub OnScriptExit()
    dm.UnBindWindow 
End Sub

Sub OnThreadExit()
    dm.UnBindWindow 
End Sub

Event Form1.Button1.Click
	霸肋門德ID=BeginThread(霸肋門德)

End Event

Event Form1.Button2.Click
	StopThread 霸肋門德ID
