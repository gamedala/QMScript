Sub 腳本
	Call set_dm
	Delay 100
	時間檢查(1)
	Do
		地圖 = Plugin.Memory.Read32Bit(Hwnd, &H0015F25C)
		//TracePrint 地圖
		//MsgBox 地圖 & Form1.打寶地圖.Text
		If 地圖 = Form1.打寶地圖.Text OR Form1.打寶地圖.Text = "0" Then
			Call 異常
    		Call hp_mp
    		Call 負重檢查
			Call verifyGo
			Call 自動輔助
    		ret = dm.FindColorEx(250, 130, 770, 570, "ffff55-000005", 1.0, 0)
    		ret = dm.FindNearestPos(ret, 1, 510, 350)
    		If ret <> "" OR Form1.技能類型.ListIndex = "1" Then 
    			If Form1.技能類型.ListIndex = "0" Then 
					arr = Split(ret, ",")
					intX = arr(0)
					intY = arr(1)
					//call RealMove(intX,intY)
       				TracePrint "[找到指定顏色]: [X: " & intX & ", Y: " &  intY & "]"
       				If (intX<> setX) AND (intY <> setY) Then 
           				時間檢查(1)
       					TracePrint "[設置]: [setX: " & setX & ", setY: " &  setY & "]"
       				ElseIf 初始 <= 時間檢查(0) Then 
           				TracePrint "[尋找超時]"
           				dm.KeyPress Form1.瞬移按鍵.Text
           				Delay 500
           				時間檢查(2)
       				End If
    			End If
       			If int(Form1.技能按鍵.Text) > 0 Then 
       				dm.KeyPress Form1.技能按鍵.Text
       				Delay Form1.技能延遲.Text
       			End If
       			If Form1.技能類型.ListIndex = "0" Then 
       				dm.MoveTo arr(0), arr(1)
       				dm.LeftClick 
       				dm.MoveTo 1024, 768
       			ElseIf Form1.技能類型.ListIndex = "1" Then
       				dm.KeyPress Form1.瞬移按鍵.Text
       			End If
       			Delay Form1.瞬移後延.Text
   			ElseIf 尋找 = int(Form1.尋找次數.Text) Then  
				TracePrint "[找不到瞬移]"
				dm.KeyPress Form1.瞬移按鍵.Text
				Delay 500
				時間檢查(2)
   			Else
       			尋找 = 尋找 + 1
       			Delay Form1.尋找延遲.Text
   			End If
    	Else 
    		Delay 1000
    	End If
	Loop
End Sub

Function 時間檢查(Ttype)
    Set sc = CreateObject("MSScriptControl.ScriptControl")
    sc.Language = "JavaScript"
    If Ttype = 0 Then 
        時間檢查 = sc.eval ("new Date().getTime();")
    ElseIf Ttype = 1 Then
		setX = intX
		setY = intY
		尋找 = 0
        初始 = sc.eval("new Date().getTime();") + Form1.發呆時間.Text '500
    ElseIf Ttype = 2 Then
		setX = 0
		setY = 0
		尋找 = 0
        初始 = sc.eval("new Date().getTime();") + Form1.發呆時間.Text '500
    End If
End Function

Function hp_mp
	If int(Form1.MP按鍵.Text) > 0 Then 
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A20))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A1C))
    	魔力 = fix((最小 / 最大) * 100)
    	If 魔力 < int(Form1.MP.Text)Then 
        	dm.KeyPress Form1.MP按鍵.Text
    	End If
	End If
	If int(Form1.HP按鍵.Text) > 0 Then 
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A18))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
    	血量 = fix((最小 / 最大) * 100)
    	If 最小 = 1 And Form1.復活選項.ListIndex <> 0 Then
        	復活(Form1.復活選項.ListIndex)
    	ElseIf 血量 < int(Form1.HP.Text) Then
        	dm.KeyPress Form1.HP按鍵.Text
    	End If
	End If
End Function

Function 復活(Run)
    If Run >0 Then
        Die = array("","移動到儲存場所","在原地復")
        TracePrint "[復活]"
        Do
            dm.KeyPress 27
            Delay 250
            dm_ret = dm.FindStr(345, 435, 674, 565, Die(Run), "000000-313131", 1.0, intX, intY)
        Loop Until intX > 0 And intY > 0
        TracePrint "找到關鍵字: [移動到儲存場所]"
        If Run = 1 Then
            Do
                dm.MoveTo intX, intY
                dm.LeftClick
                dm_ret = dm.FindStr(352,289,661,435, "確認", "000000-313131", 1.0, intX2, intY2)
            Loop Until intX2 > 0 And intY2 > 0
            TracePrint "找到關鍵字: [確認]"
        ElseIf Run = 2 Then
            intX2 = intX
            intY2 = intY
        End If
        Do
            dm.MoveTo intX2, intY2
            dm.LeftClick 
            Delay 200
            血 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
        Loop Until 血 > 1
        TracePrint "確認: [復活]"
    End If
End Function

Function 負重檢查
    最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCEDC))
    最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCEE0))
    負重 = fix((最小 / 最大) * 100)
    If 負重 > 80 Then 
        Do
            romsg = Plugin.Memory.Read32Bit(Hwnd, &H00F80558)
            If romsg <> 0 Then 
                romenu = Plugin.Memory.Read32Bit(Hwnd, &H00F8055C)
                If romenu <> 0 Then
                    dm.KeyPress 40
                    Delay 50
                    dm.KeyPress 40
                    Delay 50
                    dm.KeyPress 40
                    Delay 50
                    dm.KeyPress 13
                    Delay 50
                    dm.KeyPress 13
                    Exit Do
                Else 
                    dm.KeyPress 13
                End If
            Else
                dm.KeyDown 120
            End If
        Loop
        Call 存倉()
        Call 關閉倉庫()
    End If
End Function

Function verifyGo()
	Unix = DateDiff("s", "1970/01/01 00:00:00", Now) - 28800
	If V_Unix <= Unix Then
       	dm_ret = dm.FindStr(7, 676, 350, 701, "外掛驗證裝置", "00ff00-000000", 0.95, intX, intY)
		If intX > 0 And intY > 0 Then 
			TracePrint "[發現驗證]"
    		verify = ""
    		Do
        		romsg = Plugin.Memory.Read32Bit(Hwnd, &H00F80558)
        		romenu = Plugin.Memory.Read32Bit(Hwnd, &H00F8055C)
        		If romenu <> 0 OR romsg <> 0 Then 
            		dm_ret = dm.FindStr(0, 0, 1024, 768, "驗證碼", "000000-000000", 0.95, intX, intY)
            		If intX > 0 And intY > 0 AND verify = "" Then 
                		verify = dm.Ocr(intX + 53, intY, intX + 103, intY + 10, "0000ff-000000", 1.0)
                		If verify <> "" Then 
                    		Delay 500
                    		//Call Plugin.Bkgnd.SendString(Hwnd, verify)
                    		Call 分離(verify)
                    		TracePrint "驗證碼輸入:" & verify
                    		Delay 500
                    		dm.KeyPress 13
                    		Delay 500
                    		dm.KeyPress 13
                    		TracePrint "驗證完成"
                    		V_Unix = DateDiff("s", "1970/01/01 00:00:00", Now) - 25200
                    		Exit Do
                		End If
            		End If
            		dm.KeyPress 13
            		Delay 500
        		Else 
            		dm.MoveTo 850,90
            		dm.LeftClick 
            		Delay 500
        		End If
    		Loop
		End If
   	End If
End Function

Function 分離(Text)
    n = Len(Text)
    For i = 1 To n
        y = Mid(Text, i, 1)
        dm.KeyPress 96 + y
        Delay 20
    Next
End Function

Function 存倉()
    Dim Run
    Do
        倉庫 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCF1C))
        //Rem 重新開始
        If 倉庫 > 0 Then
            Do
                存倉結束 = 1
                dm_ret = dm.FindStr(0, 0, 1024, 768, "物品欄", "000000-000000", 1, intX, intY)
                If intX > 0 And intY > 0 Then 
                    Do
                        dm.MoveTo intX2, intY2
                        dm_ret = dm.FindStr(intX + 25, intY + 25, intX + 65, intY + 55, "無物品", "ced6e6-000005", 1, intX3, intY3)
                        If intX3 < 0 And intY3 < 0 Then 
                            dm.MoveTo intX + 40, intY + 45
                            dm.KeyDown 18
                            dm.RightClick
                            dm.KeyUp 18
                        Else 
                            存倉結束 = 0
                        End If
                        TracePrint intX3& "/" & intY3
                    Loop Until intX3 > 0 And intY3 > 0
                    TracePrint "物品: 存放完成"
                    delay 10
                Else 
                    dm.KeyDown 18
                    dm.KeyPress 69
                    dm.KeyUp 18
                End If
            Loop Until (intX > 0 And intY > 0) AND (intX3 > 0 And intY3 > 0)
            Exit Do
        End If
    Loop
End Function

Function 關閉倉庫()
    倉庫 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCF1C))
    If 倉庫 > 0 Then 
        dm_ret = dm.FindStr(0, 0, 1024, 768, "個人倉庫", "000000-000000", 1, intX, intY)
        dm_ret = dm.FindStr(intX, intY, intX + 280, intY + 450, "關閉", "000000-000000", 1, intX3, intY3)
        If intX3 > 0 And intY3 > 0 Then 
            dm.MoveTo intX3, intY3
            dm.LeftClick 
            dm.KeyDown 18
            dm.KeyPress 69
            dm.KeyUp 18
            TracePrint "關閉倉庫"
        End If
    Else
        TracePrint "[ERROR] 找不到倉庫關閉"
        Delay 1000
    End If
End Function

Function 自動輔助()
	If Form1.輔助開關.ListIndex = 1 Then 
		狀態名稱 = array(Form1.狀態編號01.Text, Form1.狀態編號02.Text, Form1.狀態編號03.Text, Form1.狀態編號04.Text, Form1.狀態編號05.Text, Form1.狀態編號06.Text, Form1.狀態編號07.Text, Form1.狀態編號08.Text, Form1.狀態編號09.Text, Form1.狀態編號10.Text)
		狀態按鍵 = array(Form1.狀態按鍵01.Text, Form1.狀態按鍵02.Text, Form1.狀態按鍵03.Text, Form1.狀態按鍵04.Text, Form1.狀態按鍵05.Text, Form1.狀態按鍵06.Text, Form1.狀態按鍵07.Text, Form1.狀態按鍵08.Text, Form1.狀態按鍵09.Text, Form1.狀態按鍵10.Text)
		For i = 0 to UBOUND(狀態名稱)
			If int(狀態按鍵(i)) <> 0 OR int(狀態名稱(i)) <> 0 Then 
				call 狀態(int(狀態名稱(i)), int(狀態按鍵(i)))
			End If
		Next
	End If
End Function

Function 狀態(狀態ID,按鍵)
	do
    	buffid = Plugin.Memory.Find32Bit(Hwnd, 狀態ID, &H011D0E94, &H011D0ED8, 4)
    	If buffid = 0 Then 
        	dm.KeyPress 按鍵
        	TracePrint "補狀態: " & 狀態ID & "按鍵: " & 按鍵
    	End If
	loop Until buffid <> 0
End Function

Function 異常()
	Do
		romsg = int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
		romenu = Plugin.Memory.Read32Bit(Hwnd, &H00F8055C)
    	If romsg > 0 or romenu > 0 Then 
			TracePrint "異常"
    		關鍵字 = array("取消", "關閉")
    		For i = 0 To 2
        		dm_ret = dm.FindStr(0, 0, 1440, 900, 關鍵字(i), "000000-313131", 1, intX, intY)
        		If intX > 0 And intY > 0 Then 
        			TracePrint "關鍵"&關鍵字(i)
        			Exit For
        		End If
        	Next
        	If intX > 0 And intY > 0 Then 
            	dm.MoveTo intX, intY
            	dm.LeftClick 
            	dm.MoveTo 1024, 768
            Else 
            	dm.KeyPress 13
        	End If
            Delay 100
    	Else
        	Exit Do
    	End If
    Loop
End Function

Sub RealMove(x1, y1)
	If x1 > 0 Then 
    	Dim x, y, sudu
    	sudu = 5//每次移動的速度
    	真是移動當前x = 500 
    	真是移動當前y = 360
    	If x1 = 真是移動當前x Then 
        	For y = 真是移動當前y To y1 Step sgn(y1 - 真是移動當前y)*sudu
            	比對 x1, y
        	Next
    	ElseIf y1 = 真是移動當前y Then
        	For x = 真是移動當前x To x1 Step sgn(x1 - 真是移動當前x)*sudu
            	比對 x, y1
        	Next
    	ElseIf Abs(真是移動當前x - x1) > Abs(真是移動當前y - y1) Then
        	For x = 真是移動當前x To x1 Step sgn(x1 - 真是移動當前x)*sudu
            	比對 x, ((y1 - 真是移動當前y) / (x1 - 真是移動當前x)) * x + (y1 - (y1 - 真是移動當前y) / (x1 - 真是移動當前x) * x1)
        	Next
    	Else 
        	For y = 真是移動當前y To y1 Step sgn(y1 - 真是移動當前y)*sudu
            	比對 (y - (y1 - (y1 - 真是移動當前y) / (x1 - 真是移動當前x) * x1)) / ((y1 - 真是移動當前y) / (x1 - 真是移動當前x)), y
        	Next
    	End If
	End If
End Sub

Function 比對(x, y)
	If (x > 454 or x < 552) and (y > 285 or y < 323) Then 
		color = dm.GetColor(x,y)
		If color = "212121" OR color = "5a5a5a" Then
			dm.moveto x, y
			//MsgBox "..."
			dm.KeyPress Form1.瞬移按鍵.Text
		End If
	End If
End Function

Function set_dm()
	Set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath()
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "驗證.txt")
    Hwnd = Plugin.Window.Find(0, Form1.窗口標題.Text)
    dm_ret = dm.BindWindow(Hwnd, "gdi", "dx2", "dx", 4)
    TracePrint "Hwnd:" & Hwnd & " dm_ret:" & dm_ret
End Function

