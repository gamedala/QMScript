Sub gethwndlist
	hwnds = dm.EnumWindow(0, "", Form1.窗口標題.Text, 2 + 4 + 8 + 16)
	//hwnds = Plugin.Window.Search(Form1.窗口標題.Text)
	hwnds = split(hwnds, ",")
		列表內容 = "請選擇"
		For i = 0 To UBound(hwnds)
			帳號 = Plugin.Memory.ReadString(hwnds(i), &H011CBFE8, 10)
			If 帳號 <> "" Then 
				For ii = 1 To UBound(Split(Form1.選單.List, "|"))
					If 帳號 = Split(Split(Form1.選單.List, "|")(ii), ",")(0) Then 
						相同 = True
						Exit For
					Else 
						相同 = False
					End If
				Next
				If 相同 = True Then 
					列表內容 = 列表內容 & "|" & Split(Form1.選單.List, "|")(ii)
				Else 
					Call Plugin.Window.SetText(hwnds(i), 帳號)
					列表內容 = 列表內容 & "|" & 帳號 & "," & hwnds(i) & ",0"
				End If
			End If
		Next
		Form1.選單.List = 列表內容
		Form1.選單.ListIndex = 0
End Sub

Sub 修改列表(狀態)
    帳號選擇內容 = Split(Form1.選單.List, "|")(Form1.選單.ListIndex)
    帳號選擇位置 = Form1.選單.ListIndex
    列表內容 = "請選擇"
    For i = 1 To UBound(Split(Form1.選單.List, "|"))
        If Form1.選單.ListIndex = i Then 
            列表內容 = 列表內容 & "|" & Split(Split(Form1.選單.List, "|")(i), ",")(0) & "," & Split(Split(Form1.選單.List, "|")(i), ",")(1) & "," & 狀態'迴圈拼出下拉全字串內容並寫入線程編號到指定下拉目標第二欄內
        Else 
            列表內容 = 列表內容 & "|" & Split(Form1.選單.List, "|")(i)'迴圈補全下拉字串內容
        End If
    Next
    Form1.選單.List = 列表內容
    Form1.選單.ListIndex = 帳號選擇位置
End Sub

Sub OnThreadExit()
    dm.UnBindWindow 
End Sub

Sub OnScriptExit()
    dm.UnBindWindow 
End Sub

Event Form1.Load '初始化
	Set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath() & "光雨"
    dm_ret = dm.SetPath(base_path)
	text = Plugin.File.GetFileLength("驗證.txt")
	If text <> 3257 Then 
    	PutAttachment "c:\dm\光雨", "驗證.txt"
	End If
	Call gethwndlist
End Event

Event Form1.刷新.Click
	Call gethwndlist
End Event

Event Form1.選單.SelectChange
    帳號選擇內容 = Split(Form1.選單.List, "|")(Form1.選單.ListIndex)'下拉的所有定義字串內容
    If Split(帳號選擇內容, ",")(2) > 0 Then '如果線程編號大於0 
        Form1.啟動.Caption = "停止"
    Else 
        Form1.啟動.Caption = "啟動"
    End If
    線程 = Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(0)'取下拉字串第一欄
    Call 讀取設定(線程)
	If Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(2) = 1 or Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(2) = 0 Then 
    	Form1.啟動.Enabled = True
		Form1.暫停.Enabled = False
	Else 
    	Form1.啟動.Enabled = False
		Form1.暫停.Enabled = True
	End If
End Event

Event Form1.保存設定.Click
	線程 = Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(0)
	Call 保存設定(線程)
End Event

Event Form1.啟動.Click
	Form1.啟動.Enabled = False
	Form1.暫停.Enabled = True
	If Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(2) = 1 Then 
		線程ID = Plugin.File.readINI(線程, "線程ID", "C:\dm\光雨\打怪驗證.ini")
		ContinueThread 線程ID
		call 修改列表(線程ID)
	Else 
		線程ID = BeginThread(腳本)
		call 修改列表(線程ID)
		Call Plugin.File.WriteINI(線程, "線程ID", 線程ID, "C:\dm\光雨\打怪驗證.ini")
	End If
    
End Event

Event Form1.暫停.Click
	Form1.暫停.Enabled = False
	Form1.啟動.Enabled = True
	線程 = Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(0)'取下拉字串第一欄
	call 修改列表(1)
	線程ID = Plugin.File.readINI(線程, "線程ID", "C:\dm\光雨\打怪驗證.ini")
	//StopThread 線程ID
	PauseThread 線程ID
End Event

