Sub 腳本
	Call 大漠()
	Delay 100
	Do
		If Form1.Button2.Caption = "關閉腳本" Then 
			NPC座標 = array(int(Form1.滑鼠X.Text), int(Form1.滑鼠Y.Text))
			裝備位置 = int(Form1.選單位置.Text)
			動作類型 = int(Form1.動作類型.ListIndex)
			洞數 = int(Form1.洞數.ListIndex)
			選單洞數 = int(Form1.選單洞數.Text)
			'記憶體位置 = array(0, 18667320, 18667552, 18665464, 18666392, 18665928, 18666856, 18665696, 18666624, 18666160, 18667088, 18671960, 18672192, 18670104, 18671032, 18670568, 18671496, 18670336, 18671264, 18670800, 18671728)
			記憶體位置 = array(0, 18164608, 18164840, 18162752, 18163680, 18163216, 18164144, 18162984, 18163912, 18163448, 18164376, 18169248, 18169480, 18167392, 18168320, 18167856, 18168784, 18167624, 18168552, 18168088, 18169016)
			位置 = int(Form1.位置.ListIndex)
			物品 = Hex(記憶體位置(位置))
			精煉 = 記憶體位置(位置)+52
			一洞 = 記憶體位置(位置)-16
			二洞 = 記憶體位置(位置)-12
			三洞 = 記憶體位置(位置)-8
			四洞 = 記憶體位置(位置)-4
			Do
				If Form1.Button2.Caption = "開始腳本" Then 
					Exit Do
				End If
				call 密語()
				Form1.編號.Text = dm.ReadString(Hwnd,物品,0,10)
				If 動作類型 = 1 Then 
					精煉值 = int(Plugin.Memory.Read32Bit(Hwnd, 精煉))
					Form1.精煉值.Text = 精煉值
					If 精煉值 >= int(Form1.精煉.Text) Then 
						Do
							Call Plugin.Media.Beep(988, 100) //蜂鳴一秒	
						Loop
					End If
				ElseIf 動作類型 = 2 Then
					If 洞數 = 1 Then 
						洞 = Plugin.Memory.Read32Bit(Hwnd, 一洞)
        				Form1.一洞.Text = 洞
					ElseIf 洞數 = 2 Then 
						洞 = Plugin.Memory.Read32Bit(Hwnd, 二洞)
						Form1.二洞.Text = 洞
					ElseIf 洞數 = 3 Then 
						洞 = Plugin.Memory.Read32Bit(Hwnd, 三洞)
						Form1.三洞.Text = 洞
					ElseIf 洞數 = 4 Then 
						洞 = Plugin.Memory.Read32Bit(Hwnd, 四洞)
						Form1.四洞.Text = 洞
					End If
					If 洞 = Form1.附魔.Text Then 
						Do
							Call Plugin.Media.Beep(988, 100) //蜂鳴一秒	
						Loop
					End If
				End If
				'對話視窗 = Plugin.Memory.Read32Bit(Hwnd, &H00F80558)
				對話視窗 = Plugin.Memory.Read32Bit(Hwnd, &H00F05990)
				If 對話視窗 > 0 Then 
					If 動作類型 = 1 Then 
						dm_ret = dm.FindStr(0, 0, 1024, 768, "物品選項簡略設置", "0063ce-000000", 1.0, intX, intY)
					ElseIf 動作類型 = 2 Then 
						dm_ret = dm.FindStr(0, 0, 1024, 768, "附魔道具說明", "008cff-000000", 1.0, intX, intY)
					End If
					If intX > 0 AND intY > 0 Then 
						For i = 1 To 裝備位置
							TracePrint "裝備位置: "&裝備位置&" 按: down " & i
							dm.Keydown 40
							dm.Keyup 40
							Delay 50
						Next
						dm.KeyPress 13
					Else 
						If 動作類型 = 2 Then 
							dm_ret = dm.FindStr(0, 0, 1024, 768, "第四洞", "008cff-000000", 1.0, intX, intY)
							TracePrint 選單洞數
							If intX > 0 AND intY > 0 Then 
								If 選單洞數 > 0 Then 
									For i = 1 To 選單洞數
										TracePrint "洞數: "& 選單洞數 &" 按: down " & i
										dm.Keydown 40
										dm.Keyup 40
										Delay 50
									Next
								End If	
							End If
							dm_ret = dm.FindStr(0, 0, 1024, 768, "請繼續", "000000-000000", 1.0, intX, intY)
							If intX > 0 AND intY > 0 Then 
								dm.Keydown 40
								dm.Keyup 40
								Delay 50
							End If
						End If
						dm.KeyPress 13
					End If
				Else 
					dm.MoveTo NPC座標(0),NPC座標(1)
					dm.LeftClick
				End If
				Delay 500
			Loop
		ElseIf Form1.取得滑鼠座標.Caption = "獲取中...." then
   			Delay 3000
    		dm.GetCursorPos x,y
			Form1.滑鼠X.Text = x
			Form1.滑鼠Y.Text = y
			Form1.取得滑鼠座標.Caption = "取得滑鼠座標"
		End If
		Delay 500
	Loop
End Sub

Sub 密語()
	msgtoyou = int(Plugin.Memory.Read32Bit(Hwnd, &H00F05CAC))
	If msgtoyou > 0 And 密語數量 <> msgtoyou Then 
		密語數量 = msgtoyou
	End If 
	If  msgtoyou > 0 Then
		MsgBox "大哥有人密你啊!"
	End If
End Sub

Function 大漠()
	set dm = createobject("dm.dmsoft")
	base_path = dm.GetBasePath() & "光雨"
	dm_ret = dm.SetPath(base_path)
	dm_ret = dm.SetDict(0, "光與精煉.txt")
	Hwnd = Plugin.Window.Find(0, "Ragnarok")
	Form1.Hwnd.Text = Hwnd
	dm_ret = dm.BindWindow(Hwnd, "gdi", "dx2", "dx", 4)
End Function

Event Form1.Load
	set dm = createobject("dm.dmsoft")
    If Plugin.File.GetFileLength("c:\dm\光雨\光雨精煉.txt") = 0 Then 
		PutAttachment "c:\dm\光雨", "*.*"
	End If
	線程ID=BeginThread(腳本)
End Event

Event Form1.取得滑鼠座標.Click
	Form1.取得滑鼠座標.Caption = "獲取中...."
End Event

Sub OnScriptExit()
    dm.UnBindWindow 
End Sub

Sub OnThreadExit()
    dm.UnBindWindow 
End Sub

Event Form1.Button1.Click
	StopThread 線程ID
	線程ID = BeginThread(腳本)
End Event

Event Form1.Button2.Click
	If Form1.Button2.Caption = "開始腳本" Then 
		Form1.Button2.Caption = "關閉腳本"
	Else 
		Form1.Button2.Caption = "開始腳本"
	End If
End Event

Event Form1.位置.SelectChange
	Hwnd = int(Form1.Hwnd.Text)
	記憶體位置 = array(0, 18164608, 18164840, 18162752, 18163680, 18163216, 18164144, 18162984, 18163912, 18163448, 18164376, 18169248, 18169480, 18167392, 18168320, 18167856, 18168784, 18167624, 18168552, 18168088, 18169016)
	位置 = int(Form1.位置.ListIndex)
	物品 = dm.ReadString(Hwnd,Hex(記憶體位置(位置)),0,10)
	精煉 = Plugin.Memory.Read32Bit(Hwnd,記憶體位置(位置)+52)
	一洞 = Plugin.Memory.Read32Bit(Hwnd,記憶體位置(位置)-16)
	二洞 = Plugin.Memory.Read32Bit(Hwnd,記憶體位置(位置)-12)
	三洞 = Plugin.Memory.Read32Bit(Hwnd,記憶體位置(位置)-8)
	四洞 = Plugin.Memory.Read32Bit(Hwnd,記憶體位置(位置) - 4)
	Form1.編號.Text = 物品
	Form1.一洞.Text = 一洞
	Form1.二洞.Text = 二洞
	Form1.三洞.Text = 三洞
	Form1.四洞.Text = 四洞
	Form1.精煉值.Text = 精煉
End Event
