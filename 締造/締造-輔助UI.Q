Sub 腳本
	Call set_dm
	Delay 100
	帳號 = Plugin.Memory.ReadString(Hwnd, &H011CBFE8, 10)
	call 應用設定(帳號)
	Do
		If Form1.套用設定.Caption = 帳號 Then
			call 應用設定(帳號)
			Form1.套用設定.Caption = "套用設定"
		end If
    	Call hp_mp
		Call 自動輔助
		Delay Form1.檢查時間.Text
	Loop
End Sub

Function hp_mp
	If MP按鍵 > 0 Then 
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A20))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A1C))
    	魔力 = fix((最小 / 最大) * 100)
    	If 魔力 < MP Then 
        	dm.KeyPress MP按鍵
    	End If
	End If
	If HP按鍵 > 0 Then 
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A18))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
    	血量 = fix((最小 / 最大) * 100)
    	If 血量 < HP Then
        	dm.KeyPress HP按鍵
    	End If
	End If
End Function

Function 自動輔助()
	If 輔助開關 = 1 Then 
		狀態名稱 = array(狀態編號01, 狀態編號02, 狀態編號03, 狀態編號04, 狀態編號05, 狀態編號06, 狀態編號07, 狀態編號08, 狀態編號09, 狀態編號10)
		狀態按鍵 = array(狀態按鍵01, 狀態按鍵02, 狀態按鍵03, 狀態按鍵04, 狀態按鍵05, 狀態按鍵06, 狀態按鍵07, 狀態按鍵08, 狀態按鍵09, 狀態按鍵10)
		狀態延遲 = array(狀態延遲01, 狀態延遲02, 狀態延遲03, 狀態延遲04, 狀態延遲05, 狀態延遲06, 狀態延遲07, 狀態延遲08, 狀態延遲09, 狀態延遲10)
		For i = 0 to UBOUND(狀態名稱)
			If 狀態按鍵(i) <> 0 OR 狀態名稱(i) <> 0 Then 
				Call 狀態(狀態名稱(i), 狀態按鍵(i), 狀態延遲(i))
			End If
		Next
	End If
End Function

Function 狀態(狀態ID,按鍵,延遲)
	do
    	buffid = Plugin.Memory.Find32Bit(Hwnd, 狀態ID, &H011D0E94, &H011D0ED8, 4)
    	If buffid = 0 Then 
        	dm.KeyPress 按鍵
        	TracePrint "補狀態: " & 狀態ID & "按鍵: " & 按鍵
        	Delay 延遲
    	End If
	loop Until buffid <> 0
End Function

Function set_dm()
	Set dm = createobject("dm.dmsoft")
    Hwnd = Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(1)
    dm_ret = dm.BindWindow(Hwnd, "gdi", "dx2", "dx", 4)
    TracePrint "Hwnd:" & Hwnd & " dm_ret:" & dm_ret
End Function

