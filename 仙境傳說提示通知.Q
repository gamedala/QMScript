Sub 腳本
	Call set_dm
	Delay 100
	時間檢查 (1)
	'帳號 = Plugin.Memory.ReadString(Hwnd, &H011CBFE8, 10)
	帳號 = Plugin.Memory.ReadString(Hwnd, &H01151430, 10)
	call 應用設定(帳號)
	Do
		If Form1.套用設定.Caption = 帳號 Then
			call 應用設定(帳號)
			Form1.套用設定.Caption = "套用設定"
		end If
		'地圖 = int(Plugin.Memory.Read32Bit(Hwnd, &H0015F25C))
		地圖 = dm.ReadString(hwnd,"011513C0",0,15)
		If 地圖 = 打寶地圖 OR 打寶地圖 = 0 Then 
			離開地圖通知 = 0
			Call 異常
    		Call hp_mp
			Call verifyGo
    		Call 負重檢查
			Call 密語
    	Else 
           	If 離開地圖 = 1 and 離開地圖通知 = 0 Then 
           		離開地圖通知 = 1
          		訊息("離開綁定地圖")
         	End If
    		Delay 1000
    	End If
    	Delay 500
	Loop
End Sub

Function 時間檢查(Ttype)
    Set sc = CreateObject("MSScriptControl.ScriptControl")
    sc.Language = "JavaScript"
    If Ttype = 0 Then 
        時間檢查 = sc.eval ("new Date().getTime();")
    ElseIf Ttype = 1 Then
		setX = intX
		setY = intY
		尋找 = 0
        初始 = sc.eval("new Date().getTime();") + 發呆時間 '500
    ElseIf Ttype = 2 Then
		setX = 0
		setY = 0
		尋找 = 0
        初始 = sc.eval("new Date().getTime();") + 發呆時間 '500
    End If
End Function

Function hp_mp
	If MP按鍵 > 0 Then 
		'最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A20))
    	'最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A1C))
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H01155E20))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H01155E1C))
    	魔力 = fix((最小 / 最大) * 100)
    	If 魔力 < MP Then 
        	dm.KeyPress MP按鍵
    	End If
	End If
	If HP按鍵 > 0 Then 
		'最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A18))
    	'最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H01155E18))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H01155E14))
    	血量 = fix((最小 / 最大) * 100)
    	If 最小 = 1 And 復活選項 <> 0 Then
        	復活(復活選項)
    	ElseIf 血量 < HP Then
        	dm.KeyPress HP按鍵
    	End If
	End If
End Function

Function 復活(Run)
    If Run >0 Then
        Die = array("","移動到儲存場所","在原地復")
        TracePrint "[復活]"
    	If 死亡通知 = 1 Then
			訊息 ("人物已死亡選擇" & Die(Run))
		End If
        Do
            dm.KeyPress 27
            Delay 250
            dm_ret = dm.FindStr(345, 435, 674, 565, Die(Run), "000000-313131", 1.0, intX, intY)
        Loop Until intX > 0 And intY > 0
        TracePrint "找到關鍵字: [移動到儲存場所]"
        If Run = 1 Then
            Do
                dm.MoveTo intX, intY
                dm.LeftClick
                dm_ret = dm.FindStr(352,289,661,435, "確認", "000000-313131", 1.0, intX2, intY2)
            Loop Until intX2 > 0 And intY2 > 0
            TracePrint "找到關鍵字: [確認]"
        ElseIf Run = 2 Then
            intX2 = intX
            intY2 = intY
        End If
        Do
            dm.MoveTo intX2, intY2
            dm.LeftClick 
            Delay 200
            '血 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
            血 = int(Plugin.Memory.Read32Bit(Hwnd, &H01155E14))
        Loop Until 血 > 1
        TracePrint "確認: [復活]"
    	If 死亡通知 = 1 Then
			訊息 ("人物滿血重生")
		End If
    End If
End Function

Function 負重檢查
    '最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCEDC))
    '最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCEE0))
    最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H01152324))
    最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H01152328))
    負重 = fix((最小 / 最大) * 100)
    If 負重 > 80 Then 
        Do
            'romsg = int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
            romsg = int(Plugin.Memory.Read32Bit(Hwnd, &H00F05990))
            If romsg <> 0 Then 
               ' romenu = int(Plugin.Memory.Read32Bit(Hwnd, &H00F8055C))
                romenu = int(Plugin.Memory.Read32Bit(Hwnd, &H00F05998))
                If romenu <> 0 Then
                    dm.KeyPress 40
                    Delay 50
                    dm.KeyPress 40
                    Delay 50
                    dm.KeyPress 40
                    Delay 50
                    dm.KeyPress 13
                    Delay 50
                    dm.KeyPress 13
                    Exit Do
                Else 
                    dm.KeyPress 13
                End If
            Else
                dm.KeyDown 120
            End If
        Loop
        Call 存倉()
        Call 關閉倉庫()
    End If
End Function

Function verifyGo()
	Unix = DateDiff("s", "1970/01/01 00:00:00", Now) - 28800
	If V_Unix <= Unix Then
       	dm_ret = dm.FindStr(7, 676, 350, 701, "[外掛驗證提示]", "00ff00-000000|b5ffb5-000000", 0.95, intX, intY)
		If intX > 0 And intY > 0 Then 
			TracePrint "[發現驗證]"
			Delay 2000
			If 驗證通知 = 1 Then
				訊息("開始驗證")
			End If
    		verify = ""
    		Do
        		'romsg = int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
        		'romenu = int(Plugin.Memory.Read32Bit(Hwnd, &H00F8055C))
        		romsg = int(Plugin.Memory.Read32Bit(Hwnd, &H00F05990))
        		romenu = int(Plugin.Memory.Read32Bit(Hwnd, &H00F05998))
        		If romenu <> 0 OR romsg <> 0 Then 
            		dm_ret = dm.FindStr(0, 0, 1024, 768, "驗證碼", "000000-000000", 0.95, intX, intY)
            		If intX > 0 And intY > 0 AND verify = "" Then 
                		verify = dm.Ocr(intX + 53, intY, intX + 103, intY + 10, "0000ff-000000", 1.0)
                		If verify <> "" Then 
                    		Delay 500
                    		//Call Plugin.Bkgnd.SendString(Hwnd, verify)
                    		Call 分離(verify)
                    		TracePrint "驗證碼輸入:" & verify
                    		Delay 500
                    		dm.KeyPress 13
                    		Delay 500
                    		dm.KeyPress 13
                    		TracePrint "驗證完成"
                    		V_Unix = DateDiff("s", "1970/01/01 00:00:00", Now) - 25200
							If 驗證通知 = 1 Then
								訊息("驗證通過")
							End If
                    		Exit Do
                		End If
            		End If
            		dm.KeyPress 13
            		Delay 500
        		Else 
            		dm.MoveTo 850,90
            		dm.LeftClick 
            		Delay 500
            		dm_ret = dm.FindStr(7, 676, 350, 701, "結束后再進行", "ffffff-000000", 0.95, intX, intY)
            		If intX > 0 And intY > 0 Then 
            			訊息("驗證出現異常")
            			EndScript
            		End If
        		End If
    		Loop
		End If
   	End If
End Function

Function 分離(Text)
    n = Len(Text)
    For i = 1 To n
        y = Mid(Text, i, 1)
        dm.KeyPress 96 + y
        Delay 20
    Next
End Function

Function 存倉()
    Dim Run
	If 存放倉庫 = 1 Then
		訊息("開始存放倉庫")
	End If
    Do
        '倉庫 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCF1C))
        倉庫 = int(Plugin.Memory.Read32Bit(Hwnd, &H01152364))
        //Rem 重新開始
        If 倉庫 > 0 Then
            Do
                存倉結束 = 1
                dm_ret = dm.FindStr(0, 0, 1024, 768, "物品欄", "000000-000000", 1, intX, intY)
                If intX > 0 And intY > 0 Then 
                    Do
                        dm.MoveTo intX2, intY2
                        dm_ret = dm.FindStr(intX + 25, intY + 25, intX + 65, intY + 55, "無物品", "ced6e6-000005", 1, intX3, intY3)
                        If intX3 < 0 And intY3 < 0 Then 
                            dm.MoveTo intX + 40, intY + 45
                            dm.KeyDown 18
                            dm.RightClick
                            dm.KeyUp 18
                        Else 
                            存倉結束 = 0
                        End If
                        TracePrint intX3& "/" & intY3
                    Loop Until intX3 > 0 And intY3 > 0
                    TracePrint "物品: 存放完成"
                    delay 10
                Else 
                    dm.KeyDown 18
                    dm.KeyPress 69
                    dm.KeyUp 18
                End If
            Loop Until (intX > 0 And intY > 0) AND (intX3 > 0 And intY3 > 0)
            Exit Do
        End If
    Loop
End Function

Function 關閉倉庫()
    '倉庫 = int(Plugin.Memory.Read32Bit(Hwnd, &H011CCF1C))
    倉庫 = int(Plugin.Memory.Read32Bit(Hwnd, &H01152364))
    If 倉庫 > 0 Then 
        dm_ret = dm.FindStr(0, 0, 1024, 768, "個人倉庫", "000000-000000", 1, intX, intY)
        dm_ret = dm.FindStr(intX, intY, intX + 280, intY + 450, "關閉", "000000-000000", 1, intX3, intY3)
        If intX3 > 0 And intY3 > 0 Then 
            dm.MoveTo intX3, intY3
            dm.LeftClick 
            dm.KeyDown 18
            dm.KeyPress 69
            dm.KeyUp 18
            TracePrint "關閉倉庫"
            If 存放倉庫 = 1 Then
        		訊息("存放倉庫完成")
       		End If
        End If
    Else
        TracePrint "[ERROR] 找不到倉庫關閉"
        Delay 1000
    End If
End Function

Function 自動輔助()
	If 輔助開關 = 1 Then 
		狀態名稱 = array(狀態編號01, 狀態編號02, 狀態編號03, 狀態編號04, 狀態編號05, 狀態編號06, 狀態編號07, 狀態編號08, 狀態編號09, 狀態編號10)
		狀態按鍵 = array(狀態按鍵01, 狀態按鍵02, 狀態按鍵03, 狀態按鍵04, 狀態按鍵05, 狀態按鍵06, 狀態按鍵07, 狀態按鍵08, 狀態按鍵09, 狀態按鍵10)
		For i = 0 to UBOUND(狀態名稱)
			If 狀態按鍵(i) <> 0 OR 狀態名稱(i) <> 0 Then 
				Call 狀態(狀態名稱(i), 狀態按鍵(i))
			End If
		Next
	End If
End Function

Function 狀態(狀態ID,按鍵)
	do
    	'buffid = Plugin.Memory.Find32Bit(Hwnd, 狀態ID, &H011D0E94, &H011D0ED8, 4)
    	buffid = Plugin.Memory.Find32Bit(Hwnd, 狀態ID, &H01156294, &H01156308, 4)
    	If buffid = 0 Then 
        	dm.KeyPress 按鍵
        	TracePrint "補狀態: " & 狀態ID & "按鍵: " & 按鍵
    	End If
	loop Until buffid <> 0
End Function

Function 異常()
	Do
    	'If int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558)) > 0 or int(Plugin.Memory.Read32Bit(Hwnd, &H00F8055C)) > 0 or int(Plugin.Memory.Read32Bit(Hwnd, &H00F8051C)) > 0 Then 
    	If int(Plugin.Memory.Read32Bit(Hwnd, &H00F05990)) > 0 or int(Plugin.Memory.Read32Bit(Hwnd, &H00F05998)) > 0 or int(Plugin.Memory.Read32Bit(Hwnd, &H00F059D8)) > 0 Then 
			TracePrint "異常"
    		關鍵字 = array("取消", "關閉", "與伺服器段線")
    		For i = 0 To UBound(關鍵字)
        		dm_ret = dm.FindStr(0, 0, 1024, 768, 關鍵字(i), "000000-313131", 1, intX, intY)
        		If intX > 0 And intY > 0 Then 
        			TracePrint "關鍵" & 關鍵字(i)
        			Exit For
        		End If
        	Next
            If intX > 0 And 關鍵字(i) = "與伺服器段線" And 斷線異常 = 1 Then 
        		訊息 (關鍵字(i))
        		EndScript
        	ElseIf intX > 0 And intY > 0 Then 
            	dm.MoveTo intX, intY
            	dm.LeftClick 
            	dm.MoveTo 1024, 768
            Else 
            	dm.KeyPress 13
        	End If
        	msgtack = msgtack + 1
            Delay 100
    	Else 
            If 對話異常 = 1 and msgtack > 0 Then
        		訊息 ("解除異常窗口")
       		End If
    		msgtack = 0
        	Exit Do
    	End If
    Loop
End Function

Function 密語()
	'msgtoyou = int(Plugin.Memory.Read32Bit(Hwnd, &H00F80848))
	msgtoyou = int(Plugin.Memory.Read32Bit(Hwnd, &H00F05CAC))
	If 密語提示 = 1 And msgtoyou > 0 And 密語數量 <> msgtoyou Then 
		訊息 ("有" & msgtoyou & "個密頻視窗")
		密語數量 = msgtoyou
	End If
	If 密語停止 = 1 And msgtoyou > 0 Then 
		EndScript
	End If
End Function

Sub 訊息(文字訊息)
	Key = LINENotify
	If Key <> "" or Key <> "KEY" Then
		Set http = Createobject("WinHttp.WinHttpRequest.5.1")
		http.open "POST","https://notify-api.line.me/api/notify", False
		http.setrequestheader "content-type", "application/x-www-form-urlencoded"
		http.setrequestheader "Authorization", "Bearer " & key
		http.send "message=帳號:" & 帳號 & "---" & 文字訊息
		set http = Nothing
	End If
	If TOKEN <> "" or Key <> "TOKEN" or ID <> "" or ID <> "ID" Then
		Set http = Createobject("WinHttp.WinHttpRequest.5.1")
		http.open "POST", "https://discord.com/api/webhooks/" & ID & "/" & TOKEN, False
		http.setrequestheader "content-type", "application/x-www-form-urlencoded"
		http.send "content= <@" & 標記ID & ">```帳號:" & 帳號 & "---" & 文字訊息 & "```"
		Set http = Nothing
	End If
End Sub

Function set_dm()
	Set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath()
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "驗證.txt")
    Hwnd = Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(1)
    dm_ret = dm.BindWindow(Hwnd, "gdi", "dx2", "dx", 4)
    TracePrint "Hwnd:" & Hwnd & " dm_ret:" & dm_ret
End Function


Sub gethwndlist
	hwnds = dm.EnumWindow(0, "", Form1.窗口標題.Text, 2 + 4 + 8 + 16)
	//hwndsa = Plugin.Window.Search(Form1.窗口標題.Text)
	//MsgBox hwnds
	hwnds = split(hwnds, ",")
		列表內容 = "請選擇"
		For i = 0 To UBound(hwnds)
			'帳號 = Plugin.Memory.ReadString(hwnds(i), &H011CBFE8, 10)
			帳號 = Plugin.Memory.ReadString(hwnds(i), &H01151430, 10)
			If 帳號 <> "" Then 
				For ii = 1 To UBound(Split(Form1.選單.List, "|"))
					If 帳號 = Split(Split(Form1.選單.List, "|")(ii), ",")(0) Then 
						相同 = True
						Exit For
					Else 
						相同 = False
					End If
				Next
				If 相同 = True Then 
					列表內容 = 列表內容 & "|" & Split(Form1.選單.List, "|")(ii)
				Else 
					Call Plugin.Window.SetText(hwnds(i), 帳號)
					列表內容 = 列表內容 & "|" & 帳號 & "," & hwnds(i) & ",0"
				End If
			End If
		Next
		Form1.選單.List = 列表內容
		Form1.選單.ListIndex = 0
End Sub

Sub 修改列表(狀態)
    帳號選擇內容 = Split(Form1.選單.List, "|")(Form1.選單.ListIndex)
    帳號選擇位置 = Form1.選單.ListIndex
    列表內容 = "請選擇"
    For i = 1 To UBound(Split(Form1.選單.List, "|"))
        If Form1.選單.ListIndex = i Then 
            列表內容 = 列表內容 & "|" & Split(Split(Form1.選單.List, "|")(i), ",")(0) & "," & Split(Split(Form1.選單.List, "|")(i), ",")(1) & "," & 狀態'迴圈拼出下拉全字串內容並寫入線程編號到指定下拉目標第二欄內
        Else 
            列表內容 = 列表內容 & "|" & Split(Form1.選單.List, "|")(i)'迴圈補全下拉字串內容
        End If
    Next
    Form1.選單.List = 列表內容
    Form1.選單.ListIndex = 帳號選擇位置
End Sub

