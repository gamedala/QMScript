    Else 
        Form1.啟動.Caption = "啟動"
    End If
    線程 = Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(0)'取下拉字串第一欄
    Call 讀取設定(線程)
	If Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(2) = 1 or Split(Split(Form1.選單.List, "|")(Form1.選單.ListIndex), ",")(2) = 0 Then 
