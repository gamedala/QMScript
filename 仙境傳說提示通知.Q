    	PutAttachment "c:\dm", "驗證.txt"
	End If
	ID = BeginThread(hide_hwnd)
	//Call gethwndlist
End Event

Sub hide_hwnd()
	Set dm = createobject("dm.dmsoft")
Do
	主窗口 = dm.EnumWindow(0, "提示通知", "", 1 + 4 + 8 + 16)
	If 主窗口 > 0 Then 
		影藏 = dm.EnumWindow(主窗口, "", "AfxWnd100s", 2 + 4 + 8 + 16)
		開始窗口 = dm.EnumWindow(主窗口, "", "#32770", 2 + 4 + 8 + 16)
		開始 = dm.EnumWindow(開始窗口, "開始使用>>", "Button", 1 + 2 + 4 + 8 + 16)
		If 開始 > 0 and 影藏 > 0 Then 
			Call Plugin.Bkgnd.LeftClick(開始, 0, 0)
			Call Plugin.Window.Hide(影藏)
			Exit Do	
		End If
	End If
	Delay 100
Loop
	線程ID = GetThreadID()
	StopThread 線程ID
End Sub

