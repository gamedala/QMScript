Sub 腳本
	
	Call set_dm
	Delay 100
	道館名稱 = array("", "初階道館", "中階道館", "高階道館", "夢魘道館", "地獄級道館", "百關道館")
	X軸 = array(0,0,162,169,177,185,185)
	Do
		地圖 = Plugin.Memory.Read32Bit(Hwnd, &H0015F25C)
		If 地圖 = Form1.道館地圖.Text Then 
			Call hp_mp
			Call 自動輔助
			 
			dm_ret = dm.FindStr(3, 681, 137, 696, "道館裁判恭喜第", "00ffff-000000", 1.0, intX, intY)
			If intX > 0 Then 
				s = dm.Ocr(114, 682, 136, 695, "00ffff-000000", 1.0)
				msgs = int(s)
				Form1.關卡.Caption = msgs
			End If
			If Form1.隊長AID.Text = Plugin.Memory.Read32Bit(Hwnd, &H011CCDFC) And ( msgs = 0 or msgs = 5 or msgs = 9) And Form1.狀態.Caption <> "對話" & msgs Then 
				dm.MoveTo 510, 350
				Delay 1000
				dm_ret = dm.FindStr(414,196,580,442, "道館裁判|裁判", "94bdf7-000000", 1.0, intX, intY)
				If intX > 0 Then 
					Do
						dm.LeftClick 
					Loop While 0 = int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
					Do
						dm.KeyPress 13
						Delay 200
					Loop While 0 < int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
					Form1.狀態.Caption = "對話" & msgs
				End If
				dm.MoveTo 1024, 768
			End If 
				ret = dm.FindColorEx(250, 130, 770, 570, "ffff55-000005", 1.0, 0)
				ret = dm.FindNearestPos(ret, 1, 510, 350)
				If ret <> "" OR Form1.技能類型.ListIndex = 1 Then 
					If Form1.技能類型.ListIndex = 0 Then 
						arr = Split(ret, ",")
						intX = arr(0)
						intY = arr(1)
					End If
					If int(Form1.技能按鍵.Text) > 0 Then 
						dm.KeyPress Form1.技能按鍵.Text
					End If
					If Form1.技能類型.ListIndex = 0 Then 
						dm.MoveTo intX, intY
						dm.LeftClick 
						dm.MoveTo 1024, 768
					End If
					Delay Form1.技能延遲.Text
				End If
		ElseIf 地圖 = Form1.出館地圖.Text And (Form1.隊長AID.Text = Plugin.Memory.Read32Bit(Hwnd, &H011CCDFC) or Form1.道館選擇.ListIndex = 6) Then
			Delay 1000
			If X軸(Form1.道館選擇.ListIndex) > int(Plugin.Memory.Read32Bit(Hwnd, &H011B95B0)) Then 
				Form1.狀態.Caption = "找路"
				dm.MoveTo 500, 360
				dm.LeftDown
				dm.MoveTo 750, 360
				Do
					Delay 50
				Loop While int(Plugin.Memory.Read32Bit(Hwnd, &H011B95B0)) < X軸(Form1.道館選擇.ListIndex)
				dm.LeftUp 
				Form1.狀態.Caption = "抵達"
			ElseIf Form1.隊長AID.Text = Plugin.Memory.Read32Bit(Hwnd, &H011CCDFC) or int(Plugin.Memory.Read32Bit(Hwnd, &H011D0E1C)) > 0 then
				dm_ret = dm.FindStr(0, 0, 1024, 768, 道館名稱(Form1.道館選擇.ListIndex), "ffffff-000000", 1.0, intX, intY)
				If intX > 0 Then 
					Form1.狀態.Caption = "找到" & 道館名稱(Form1.道館選擇.ListIndex)
					Do
						dm.MoveTo intX + 27, intY + 27
						dm.LeftClick 
					Loop While 0 = int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
					dm.KeyPress 13
					Delay 200
					dm.KeyPress 13
					Delay 200
					If int(Plugin.Memory.Read32Bit(Hwnd, &H011D0E1C)) > 0 Then 
						dm.KeyPress 40
						Delay 50
					End If
					Do
						dm.KeyPress 13
						Delay 200
					Loop While 0 < int(Plugin.Memory.Read32Bit(Hwnd, &H00F80558))
					msgs = 0
					Form1.狀態.Caption = "進入" & 道館名稱(Form1.道館選擇.ListIndex)
					Delay Form1.入館後延.Text
				End If
			End If
		End If
	Loop
End Sub

Function hp_mp
	If int(Form1.MP按鍵.Text) > 0 Then 
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A20))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A1C))
    	魔力 = fix((最小 / 最大) * 100)
    	If 魔力 < int(Form1.MP.Text)Then 
        	dm.KeyPress Form1.MP按鍵.Text
    	End If
	End If
	If int(Form1.HP按鍵.Text) > 0 Then 
		最大 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A18))
    	最小 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
    	血量 = fix((最小 / 最大) * 100)
    	If 最小 = 1 And Form1.復活選項.ListIndex <> 0 Then
        	復活(Form1.復活選項.ListIndex)
    	ElseIf 血量 < int(Form1.HP.Text) Then
        	dm.KeyPress Form1.HP按鍵.Text
    	End If
	End If
End Function

Function 復活(Run)
    If Run >0 Then
        Die = array("","移動到儲存場所","在原地復")
        TracePrint "[復活]"
        Do
            dm.KeyPress 27
            Delay 250
            dm_ret = dm.FindStr(345, 435, 674, 565, Die(Run), "000000-313131", 1.0, intX, intY)
        Loop Until intX > 0 And intY > 0
        TracePrint "找到關鍵字: [移動到儲存場所]"
        If Run = 1 Then
            Do
                dm.MoveTo intX, intY
                dm.LeftClick
                dm_ret = dm.FindStr(352,289,661,435, "確認", "000000-313131", 1.0, intX2, intY2)
            Loop Until intX2 > 0 And intY2 > 0
            TracePrint "找到關鍵字: [確認]"
        ElseIf Run = 2 Then
            intX2 = intX
            intY2 = intY
        End If
        Do
            dm.MoveTo intX2, intY2
            dm.LeftClick 
            Delay 200
            血 = int(Plugin.Memory.Read32Bit(Hwnd, &H011D0A14))
        Loop Until 血 > 1
        TracePrint "確認: [復活]"
    End If
End Function

Function 自動輔助()
	If Form1.狀態開關.ListIndex = 1 Then 
		狀態名稱 = array(Form1.狀態編號01.Text, Form1.狀態編號02.Text, Form1.狀態編號03.Text, Form1.狀態編號04.Text, Form1.狀態編號05.Text, Form1.狀態編號06.Text, Form1.狀態編號07.Text, Form1.狀態編號08.Text, Form1.狀態編號09.Text, Form1.狀態編號10.Text)
		狀態按鍵 = array(Form1.狀態按鍵01.Text, Form1.狀態按鍵02.Text, Form1.狀態按鍵03.Text, Form1.狀態按鍵04.Text, Form1.狀態按鍵05.Text, Form1.狀態按鍵06.Text, Form1.狀態按鍵07.Text, Form1.狀態按鍵08.Text, Form1.狀態按鍵09.Text, Form1.狀態按鍵10.Text)
		For i = 0 to UBOUND(狀態名稱)
			If int(狀態按鍵(i)) <> 0 And int(狀態名稱(i)) <> 0 Then 
				call 狀態(int(狀態名稱(i)), int(狀態按鍵(i)))
			End If
		Next
	End If
End Function

Function 狀態(狀態ID,按鍵)
	do
    	buffid = Plugin.Memory.Find32Bit(Hwnd, 狀態ID, &H011D0E94, &H011D0ED8, 4)
    	If buffid = 0 Then 
        	dm.KeyPress 按鍵
        	TracePrint "補狀態: " & 狀態ID & "按鍵: " & 按鍵
    	End If
	loop Until buffid <> 0
End Function

Function set_dm()
	Set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath()
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "關鍵字.txt")
    Hwnd = Plugin.Window.Find(0, Form1.窗口標題.Text)
    dm_ret = dm.BindWindow(Hwnd, "gdi", "dx2", "dx", 4)
    TracePrint "Hwnd:" & Hwnd & " dm_ret:" & dm_ret
End Function

Sub OnThreadExit()
    dm.UnBindWindow 
End Sub

