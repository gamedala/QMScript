[Script]
Dimenv Hwnds, 補血魔按鍵, 補血魔, 狀態編號, 狀態按鍵, 啟動地圖, 驗證延遲, 順移

Sub 打字
	句柄 = Hwnds
	Call set_dm
	Delay 100
	MyArray = Split(Form1.InputBox5.Text&Form1, "|")'分割字串
	MsgBox MyArray
	Do
		If int(Plugin.Memory.Read32Bit(句柄, &H0015F25C)) = 啟動地圖 Then 
    		Unix = DateDiff("s", "1970/01/01 00:00:00", Now) - 28800
    		if V_Unix <= Unix Then
        		dm_ret = dm.FindStr(5, 718, 355, 738, "外掛驗證裝置", "00ff00-000000", 1.0, intX, intY)
        		If intX > 0 And intY > 0 Then 
            		TracePrint "發現驗證"
            		Call verifyGo()
        		End If
    		End If
			For i = 0 to UBOUND(技能按鍵)
				dm.KeyPress 技能按鍵(i)
				Delay 技能延遲(i)
			Next
			dm.KeyPress 順移
			Delay 100
		End If
	Loop
End Sub

Sub Hindsight
	句柄 = Hwnds
	set dm = createobject("dm.dmsoft")
	dm_ret = dm.BindWindow(句柄, "gdi", "dx2", "dx", 4)
	arr1 = 狀態編號
	arr2 = 狀態按鍵
	arr3 = 補血魔按鍵
	arr4 = 補血魔
	Delay 100
	Do
		Call HP
		If int(Plugin.Memory.Read32Bit(句柄, &H0015F25C)) = 啟動地圖 Then 
			For i = 0 to UBOUND(arr1)
				If arr2(i) <> 0 Then 
					call 狀態(arr1(i), arr2(i))
				End If
			Next
			Delay 2000
		End If
		Delay 100
	Loop
End Sub

Function verifyGo()
    TracePrint "開始驗證"
    verify = ""
    Do
        romsg = Plugin.Memory.Read32Bit(句柄, &H00F80558)
        romenu = Plugin.Memory.Read32Bit(句柄, &H00EC070C)
        If romenu <> 0 OR romsg <> 0 Then 
            dm_ret = dm.FindStr(0, 0, 1024, 768, "驗證碼", "000000-000000", 0.95, intX, intY)
            If intX > 0 And intY > 0 AND verify = "" Then 
                verify = dm.Ocr(intX + 53, intY, intX + 103, intY + 10, "0000ff-000000", 1.0)
                If verify <> "" Then 
                    Delay 500
                    //Call Plugin.Bkgnd.SendString(Hwnd, verify)
                    Call list_mid(verify)
                    TracePrint "驗證碼輸入:" & verify
                    Do
                    	dm.KeyPress 13
                    	Delay 100
                    	romsg = Plugin.Memory.Read32Bit(句柄, &H00EC0708)
                    Loop While romsg > 0
                    V_Unix = DateDiff("s", "1970/01/01 00:00:00", Now) - (28800-驗證延遲)
                    TracePrint "驗證完成"
                    Exit Do
                End If
            End If
            dm.KeyPress 13
            Delay 500
        Else 
            dm.KeyPress 驗證按鍵
            Delay 500
        End If
    Loop
End Function

Function 狀態(ID,Key)
	do
		buffid = Plugin.Memory.Find32Bit(句柄, ID, &H011D0E94, &H011D0ED8, 4)
		If buffid = 0 Then 
			dm.KeyPress Key
			TracePrint "補狀態: " & ID & "按鍵: " & Key
		End If
	loop Until buffid <> 0
End Function

Function HP()
	If arr4(0) <> 0 Then 
		M = int(Plugin.Memory.Read32Bit(句柄, &H011D0A18))
		N = int(Plugin.Memory.Read32Bit(句柄, &H011D0A14))
		HP = fix((N / M) * 100)
		if HP < arr4(0) Then
			dm.KeyPress arr3(0)
		End If
	End If
	If arr4(1) <> 0 Then 
		M = int(Plugin.Memory.Read32Bit(句柄, &H011D0A1C))
		N = int(Plugin.Memory.Read32Bit(句柄, &H011D0A20))
		MP = fix((N / M) * 100)
		if MP < arr4(1) Then
			dm.KeyPress arr3(1)
		End If
	End If
End Function

Function list_mid(Text)
    n = Len(Text)
    For i = 1 To n
        y = Mid(Text, i, 1)
        dm.KeyPress 96 + y
        Delay 20
    Next
End Function

Function tipram()
	數值 = array( 17895348, 17895352, 17895356, 17895360, 17895364, 17895368, 17895372, 17895376, 17895380, 17895384, 17895388, 17895392, 17895396, 17895400, 17895404, 17895408, 17895412, 17895416, 17895420, 17895424 )
    TracePrint "地圖編號: " & int(Plugin.Memory.Read32Bit(句柄, &H0110C874))
    For i = 0 To UBOUND(數值)
    	位址 = "&H0" & Hex(數值(i))
        TracePrint "狀態[" & i & "] " & int(Plugin.Memory.Read32Bit(句柄, 位址))
    Next
End Function

sub set_dm()
    set dm = createobject("dm.dmsoft")
    base_path = dm.GetBasePath()
    dm_ret = dm.SetPath(base_path)
    dm_ret = dm.SetDict(0, "蒼月驗證.txt")
    dm_ret = dm.BindWindow(句柄, "gdi", "dx2", "dx", 4)
End sub

Event Form1.Load
	strout=""
	Set objWMIService = GetObject("winmgmts:\\.\root\CIMV2")
	Set colItems = objWMIService.ExecQuery("Select * from Win32_Processor")
	str=""
	str=str & vbcrlf & "For Each objItem in colItems"
	str = str & vbcrlf & " strOut = strOut &objItem.ProcessorId "
	str=str & vbcrlf &"Next"
	execute(str)
	TracePrint "序列號:" & strOut
//	If strOut = "BFEBFBFF000206A7" Then
//    	TracePrint "代???里"
//    	Else
//	MessageBox    "聯繫作者"&strOut
//	End If

	Call gethwndlist
	Form1.Button2.Enabled = true
	Form1.Button3.Enabled = true
	Form1.InputBox10.Text = strOut
End Event 

Event Form1.Button1.Click
	Form1.Button1.Enabled = False
    Hwnd = Plugin.Window.Search("Ragnarok")'搜索標題指定句柄  
	MyArray = Split(Hwnd, "|")'分割字串
	If UBound(MyArray) >= 0 Then
		For i = 0 To UBound(MyArray) - 1
			Hwnds = Clng(MyArray(i))
			Call Plugin.Window.SetText(Hwnds, "仙境傳說-" & i + 1)
			線程ID = BeginThread(打字)
        	Call Plugin.File.WriteINI("窗口句柄", "仙境傳說-" & i+1, Hwnds, "C:\記錄.ini")
        	Call Plugin.File.WriteINI("線程ID", "仙境傳說-" & i+1, 線程ID, "C:\記錄.ini")
        	Form1.Label4.Caption = i+1
		Next
	End If
	Form1.Label6.Caption = 0
	Form1.Label8.Caption = 0
	Form1.Button2.Enabled = true
	Form1.Button3.Enabled = true
End Event

Event Form1.Button2.Click 
	If Form1.Button2.Caption = "全部暫停" Then
		For i = 1 To cint(Form1.Label4.Caption)
			暫停線程id = Plugin.File.readINI("線程ID", "仙境傳說-" & i, "C:\記錄.ini")
			PauseThread 暫停線程id
			Form1.Label6.Caption = i
		Next
        Form1.Button2.Caption = "全部繼續"
    else
		For i = 1 To cint(Form1.Label4.Caption)
			繼續線程id = Plugin.File.readINI("線程ID", "仙境傳說-" & i, "C:\記錄.ini")
			ContinueThread 繼續線程id
		Next
		Form1.Label6.Caption = 0
        Form1.Button2.Caption = "全部暫停"
    End If
End Event

Event Form1.Button3.Click
    Form1.Button3.Enabled = False
    Form1.Button2.Enabled = False
    For i = 1 To cint(Form1.Label4.Caption)
        關閉線程id = Plugin.File.readINI("線程ID", "仙境傳說-" & i, "C:\記錄.ini")
        StopThread 關閉線程id //關閉線程
        Call Plugin.Window.Close(Plugin.File.readINI("窗口句柄", "仙境傳說-" & i, "C:\記錄.ini")) //關閉窗口
		Form1.Label8.Caption = i
    Next
	Form1.Label4.Caption = 0
	Form1.Label6.Caption = 0
	Form1.Button1.Enabled = true
End Event

Event Form1.Button4.Click
	If Plugin.File.GetFileLength("c:\dm\蒼月驗證.txt") = 0 Then 
		PutAttachment "c:\dm", "*.*"
		MsgBox "已釋放字點檔至 c:\dm"
	Else 
		MsgBox "您的 c:\dm 已經有字點檔"
	End If
End Event

